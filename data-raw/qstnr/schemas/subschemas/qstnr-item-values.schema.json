{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://qstnr.rpkg.dev/dev/schemas/subschemas/qstnr-item-values.schema.json",
  "title": "qstnr item values specification",
  "description": "item values",
  "type": "object",
  "properties": {
    "int": {
      "type": "array",
      "items": { "type": "integer" },
      "minItems": 1,
      "uniqueItems": true
    },
    "targets": { "$ref": "https://qstnr.rpkg.dev/dev/schemas/subschemas/qstnr-item-targets.schema.json" }
  },
  "patternProperties": {
    "^[a-z]{2}$": {
      "type": "array",
      "items": {
        "type": "string",
        "minLength": 1
      },
      "minItems": 1,
      "uniqueItems": true
    }
  },
  "required": ["int"],
  "additionalProperties": false
}
